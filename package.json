{
  "name": "no-logs",
  "displayName": "No-Logs",
  "description": "Scan and remove console.log across your workspace with preview.",
  "version": "0.0.1",
  "publisher": "no-logs",
  "license": "MIT",
  "engines": {
    "vscode": "^1.88.0"
  },
    "repository": {
    "type": "git",
    "url": "https://github.com/526PHANI/no_logs.git"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onCommand:noLogs.scan",
    "onCommand:noLogs.clean"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "noLogs.scan",
        "title": "No-Logs: Preview console.log occurrences"
      },
      {
        "command": "noLogs.clean",
        "title": "No-Logs: Remove console.log after review"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "package": "vsce package",
    "test": "echo \"No tests\""
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.88.0",
    "typescript": "^5.6.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "acorn": "^8.15.0",
    "acorn-typescript": "^1.4.13"
  }
}
